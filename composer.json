{
  "name": "imbrett/appname",
  "description": "Your description",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "GraphQL",
    "lighthouse"
  ],
  "type": "library",
  "readme": "README.md",
  "license": "MIT",
  "authors": [
    {
      "name": "ImBrett",
      "email": "ImBrett@example.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "nuwave/lighthouse": "^4.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "^4.5",
    "squizlabs/php_codesniffer": "^3.5",
    "theseer/phpdox": "^0.12.0",
    "phploc/phploc": "^5.0",
    "jaschilz/php-coverage-badger": "^2.0"
  },
  "scripts": {
    "post-install-cmd": [
      "bash git-hooks/install.sh"
    ],
    "lint": "\"vendor/bin/phpcs\"",
    "lint:fix": "\"vendor/bin/phpcbf\"",
    "test": "\"vendor/bin/phpunit\"",
    "gen:loc": "\"vendor/bin/phploc\" --count-tests src/ tests/ --log-xml phploc.xml",
    "gen:api": "\"vendor/bin/phpdox\"",
    "gen:badge": "\"vendor/bin/php-coverage-badger\" build/logs/clover.xml ./cover-badge.svg",
    "build": [
      "@composer lint",
      "@composer test",
      "@composer gen:loc",
      "@composer gen:api",
      "@composer gen:badge"
    ]
  },
  "autoload": {
    "psr-4": {
      "ImBrett\\AppName\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ImBrett\\AppName\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "ImBrett\\AppName\\Providers\\AppNameServiceProvider"
      ]
    }
  }
}
